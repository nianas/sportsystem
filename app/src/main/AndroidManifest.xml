<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.backet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CreateTeamActivity"></activity>
        <activity android:name=".TeamListActivity" />
        <activity
            android:name=".TeamsActivity"
            android:label="@string/title_activity_teams"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ApplicationActivity" />
        <activity android:name=".NewMatchActivity" />
        <activity android:name=".ScheduleActivity" />
        <activity android:name=".StandingsActivity" />
        <activity android:name=".CreateMatchActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>